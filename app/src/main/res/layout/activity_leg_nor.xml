                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="30dp"
                android:text="다리 중급"
                android:textSize="40dp"
